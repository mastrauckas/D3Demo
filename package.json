{
  "name": "d3-demo",
  "version": "1.0.0",
  "description": "A demo of d3 framework.",
  "main": "webpack.config.js",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "npm run build:dev:watch",
    "build:dev:watch": "npm run clean && cross-env NODE_ENV=development webpack --progress --profile --color --watch",
    "build:dev:nowatch": "npm run clean && cross-env NODE_ENV=development webpack --progress --profile --color",
    "build:prod": "npm run clean c && cross-env NODE_ENV=production webpack --progress --profile --color",
    "dev-server": "npm run dev-server:local",
    "dev-server:local": "npm run lint && cross-env NODE_ENV=development webpack-dev-server",
    "test": "npm run test:javascript",
    "lint": "npm run lint:javascript",
    "lint:javascript": "eslint ssrc",
    "test:javascript": "",
    "clean": "rimraf ./build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastrauckas/d3-demo.git"
  },
  "author": "Michael A Astrauckas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mastrauckas/d3-demo/issues"
  },
  "homepage": "https://github.com/mastrauckas/d3-demo#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.16.0",
    "eslint-loader": "^1.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "d3": "^4.7.4"
  }
}
